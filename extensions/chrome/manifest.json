{
  "manifest_version": 3,
  "name": "WDOC File Reader",
  "version": "0.1",
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Open WDOC Viewer",
    "default_popup": "app.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["libs/jszip.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
